{
  "name": "remix-worker",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "sbuild": "shopify hydrogen build --codegen",
    "deploy": "npm run build && wrangler deploy",
    "dev": "remix vite:dev",
    "Sdev": "shopify hydrogen dev --codegen",
    "s-typegen": "sanity schema extract --path=./app/sanity/extract.json && sanity typegen generate --schema=./app/sanity/extract.json && rm ./app/sanity/extract.json",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler dev",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler dev",
    "cf-typegen": "wrangler types",
    "codegen": "shopify hydrogen codegen",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply test-accepted --local",
    "db:migrate:prod": "wrangler d1 migrations apply test-accepted --remote",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview test-accepted --remote",
    "db:studio:local": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "source .drizzle.env && DB_ID='yyyyyyyy-yyyy-yyyy-yyyy-yyyyyyyyyyyy' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.3",
    "@heroicons/react": "^2.2.0",
    "@imagekit/react": "^5.0.1",
    "@remix-run/cloudflare": "^2.16.6",
    "@remix-run/fs-routes": "^2.16.6",
    "@remix-run/react": "^2.16.6",
    "@remix-run/route-config": "^2.16.6",
    "@remix-run/server-runtime": "^2.16.6",
    "@sanity/icons": "^3.7.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-url-secret": "^2.1.11",
    "@sanity/react-loader": "^1.11.10",
    "@sanity/ui": "^2.15.18",
    "@sanity/visual-editing": "^2.13.22",
    "@shopify/hydrogen": "^2025.1.4",
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.7",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "composable-functions": "^5.0.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "feed": "^5.0.1",
    "framer-motion": "^12.11.3",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "groq": "^3.88.3",
    "imagekit": "^6.0.0",
    "isbot": "^4.1.0",
    "js-sha256": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-measure": "^2.1.7",
    "sanity": "^3.89.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250514.0",
    "@graphql-codegen/cli": "5.0.2",
    "@remix-run/dev": "^2.16.6",
    "@sanity/client": "^7.3.0",
    "@shopify/cli": "^3.79.0",
    "@shopify/hydrogen-codegen": "^0.3.3",
    "@shopify/prettier-config": "^1.1.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.10.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.1.6",
    "vite": "^6.2.4",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^4.14.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
